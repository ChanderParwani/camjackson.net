    "highlight.js": "^8.9.1",
    "marked": "^0.3.5",
    "mkdirp": "^0.5.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "devDependencies": {
    "concurrently": "^5.0.1",
    "fs-extra": "^8.1.0",
    "nodemon": "^2.0.1",
    "serve": "^11.2.0"
  }
